    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.75.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.189",
    "@types/react-native": "0.71.0",
