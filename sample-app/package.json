    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "babel-jest": "^29.3.1",
    "eslint": "^7.32.0",
    "jest": "^29.3.1",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
