    "lint": "tsc && eslint '*/**/*.{js,ts,tsx}' --quiet"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.3",
    "@react-native-async-storage/async-storage": "^1.17.11",
