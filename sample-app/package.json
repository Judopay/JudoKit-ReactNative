    "judokit-react-native": "file:../",
    "lodash": "^4.17.21",
    "react": "18.0.0",
    "react-native": "0.69.7",
    "react-native-dialog": "^9.3.0",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-reanimated": "^2.13.0",
